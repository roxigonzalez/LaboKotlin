<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/billboardFragment3">

    <fragment
        android:id="@+id/billboardFragment3"
        android:name="com.rgonzalez.billboard_movie.BillboardFragment"
        android:label="fragment_billboard"
        tools:layout="@layout/fragment_billboard" >
        <action
            android:id="@+id/action_billboardFragment3_to_movieFragment"
            app:destination="@id/movieFragment" />
        <action
            android:id="@+id/action_billboardFragment3_to_newMovieFragment"
            app:destination="@id/newMovieFragment" />
    </fragment>
    <fragment
        android:id="@+id/movieFragment"
        android:name="com.rgonzalez.billboard_movie.MovieFragment"
        android:label="fragment_movie"
        tools:layout="@layout/fragment_movie" />
    <fragment
        android:id="@+id/newMovieFragment"
        android:name="com.rgonzalez.billboard_movie.NewMovieFragment"
        android:label="fragment_new_movie"
        tools:layout="@layout/fragment_new_movie" />
</navigation>